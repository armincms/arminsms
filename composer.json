{
    "name": "armincms/arminsms",
    "description": "Armin SMS configuration",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/qasedak": "~0.1",
        "armincms/bios": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Arminsms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Arminsms\\ArminsmsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
